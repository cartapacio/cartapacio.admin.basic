<div class="col-md-12 projects">
  <div class="col-md-2">
    <h4><i class="fa fa-file-text-o"></i> projects</h4>
    <div id="new" class="new"><i class="fa fa-plus-square-o"></i></div>
  </div>
    <div class="col-md-10 right">
      <ul id="project-list">
        {{#each models}}
          <li id="{{this.id}}">
            <span class="project-title">
              <a class="detail" data-id="{{this.id}}" href="#project/{{this.id}}">{{this.attributes.title}}</a>
            </span>

            <span class="delete" data-id="{{this.id}}">
              <i class="fa fa-eraser"></i>
            </span>

            <span class="move"> <i class="fa fa-arrows-v"></i></span>
          </li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>
